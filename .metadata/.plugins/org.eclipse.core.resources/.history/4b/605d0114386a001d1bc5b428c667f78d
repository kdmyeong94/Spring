 package com.koreait.core2.member;

import javax.persistence.Entity;
import javax.persistence.Id;

//@Entity: jpa가 관리하는 class
@Entity
public class Member {
	
	@Id
	private int id;
	private String name;
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
}
